<div class="partners_home">
    <div class="partners_content">
        <h3>{{ 'block.home.partner.title'|trans({},'Block') }}</h3>
        <div class="partners_txt">
            <a id="partners_prev" class="jcarousel_partners-prev" href="#" title="{{ 'block.home.partner.carousel.previous.title'|trans({},'Block') }}"></a><a id="partners_next" class="jcarousel_partners-next" href="#" title="{{ 'block.home.partner.carousel.next.title'|trans({},'Block') }}"></a>
            <div id="partners">
                {% for row in rows %}
                    <div>
                        {% if row.image %}
                            <img src="{% path row.image, 'partner_home' %}" alt="" />
                        {% endif %}
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>
</div>
